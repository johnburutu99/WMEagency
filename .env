# .env is better suited for public variables, ie, variables that should not commited
# For secret variables is better to use DevServerControl tool with set_env_variable: ["KEY", "SECRET"]

# https://www.builder.io/c/docs/using-your-api-key
VITE_PUBLIC_BUILDER_KEY=__BUILDER_PUBLIC_KEY__
PING_MESSAGE="ping pong"

# Admin credentials and JWT Secret
ADMIN_USER=admin
ADMIN_PASSWORD=admin123
JWT_SECRET=a-secure-and-random-secret-key-for-jwt

# Email credentials - Replace with your actual credentials
EMAIL_HOST=smtp.example.com
EMAIL_PORT=465
EMAIL_USER=user@example.com
EMAIL_PASSWORD=your_email_password
EMAIL_SSL=true
EMAIL_AUTH_TYPE=PLAIN
